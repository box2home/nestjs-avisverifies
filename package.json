{
    "name": "nestjs-avisverifies",
    "version": "1.0.0",
    "private": true,
    "description": "NestJS module integrating Avis Verifiés",
    "author": "Oussama Abdallah <oussama.abdallah@box2home.fr>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/box2home/nestjs-avisverifies"
    },
    "bugs": "https://github.com/box2home/nestjs-avisverifies/issues",
    "readmeFilename": "README.md",
    "main": "dist/index.js",
    "files": [
        "dist/**/*",
        "*.md"
    ],
    "scripts": {
        "release": "npx -p @semantic-release/changelog -p @semantic-release/git@beta -p semantic-release@beta  semantic-release --no-ci",
        "commit": "npx git-cz",
        "start:dev": "tsc -w",
        "prebuild": "rm -rf dist",
        "build": "tsc -p tsconfig.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint src/**/*.ts --quiet",
        "lint:fix": "eslint src/**/*.ts --quiet --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare": "npm run build"
    },
    "keywords": [
        "nestjs",
        "Avis Verifiés Module"
    ],
    "peerDependencies": {
        "@nestjs/common": "^7.0.0",
        "@nestjs/core": "^7.0.0"
    },
    "dependencies": {
        "@nestjs/common": "^7.0.7",
        "@nestjs/core": "^7.0.7",
        "@nestjs/platform-express": "^7.0.7",
        "querystring": "^0.2.0",
        "sha1": "^1.1.1",
        "tslib": "^1.11.1",
        "rxjs": "^6.5.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.2.0",
        "@commitlint/config-conventional": "^8.2.0",
        "@nestjs/testing": "^7.0.7",
        "@types/express": "^4.17.0",
        "@types/jest": "^24.0.13",
        "@types/node": "^12.0.7",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.17.0",
        "cz-conventional-changelog": "^3.1.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.19.1",
        "husky": "^4.1.0",
        "jest": "^25.2.4",
        "lint-staged": "^10.1.1",
        "prettier": "^1.18.2",
        "ts-jest": "^25.3.0",
        "ts-node": "^8.1.0",
        "tsc-watch": "^4.2.3",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.8.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "lint-staged": {
        "*.@(ts)": [
            "tslint --fix -p .",
            "git add"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run build && lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
