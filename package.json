{
  "name": "nestjs-avisverifies",
  "version": "1.0.0",
  "private": true,
  "description": "NestJS module integrating Avis Verifiés",
  "author": "Oussama Abdallah <oussama.abdallah@box2home.fr>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/box2home/nestjs-avisverifies"
  },
  "bugs": "https://github.com/box2home/nestjs-avisverifies/issues",
  "readmeFilename": "README.md",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "release": "npx -p @semantic-release/changelog -p @semantic-release/git@beta -p semantic-release@beta  semantic-release --no-ci",
    "commit": "npx git-cz",
    "start:dev": "tsc -w",
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts --quiet",
    "lint:fix": "eslint src/**/*.ts --quiet --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "npm run build"
  },
  "keywords": [
    "nestjs",
    "Avis Verifiés Module"
  ],
  "peerDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.12",
    "@nestjs/core": "^7.6.12",
    "@nestjs/platform-express": "^9.2.1",
    "node-notifier": "^8.0.1",
    "querystring": "^0.2.0",
    "rxjs": "^6.6.3",
    "sha1": "^1.1.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@nestjs/testing": "^7.6.12",
    "@types/express": "^4.17.11",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "jest": "^25.2.4",
    "lint-staged": "^10.5.4",
    "prettier": "^1.19.1",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.10.2",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.@(ts)": [
      "tslint --fix -p .",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
